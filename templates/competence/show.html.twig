{% extends 'base.html.twig' %}

{% block title %}Competence{% endblock %}

{% block body %}
    <h1>Competence</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ competence.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ competence.title }}</td>
            </tr>
            <tr>
                <th>Actif</th>
                <td>{{ competence.actif ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ competence.createdAt ? competence.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Modified_at</th>
                <td>{{ competence.modifiedAt ? competence.modifiedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Deleted_at</th>
                <td>{{ competence.deletedAt ? competence.deletedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('competence_index') }}">back to list</a>

    <a href="{{ path('competence_edit', {'id': competence.id}) }}">edit</a>

    {{ include('competence/_delete_form.html.twig') }}
{% endblock %}
